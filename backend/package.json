{
  "name": "college-database-backend",
  "version": "1.0.0",
  "description": "College Database Backend with SQLite and Cloudflare integration",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "sqlite:setup": "node sqlite-setup.js",
    "sqlite:migrate": "node sqlite-migrate.js",
    "sqlite:migrate:corrected": "node sqlite-migrate-fixed.js",
    "sqlite:cli": "node sqlite-cli.js",
    "sqlite:test": "node -e \"const SQLiteSearchEngine = require('./sqlite-search'); const engine = new SQLiteSearchEngine(); engine.initialize().then(() => { console.log('âœ… SQLite test successful'); engine.close(); }).catch(console.error);\"",
    "build": "echo 'Cloudflare Worker ready for deployment'",
    "worker:deploy": "wrangler deploy --env production",
    "worker:dev": "wrangler dev",
    "counselling:import": "node counselling-import.js",
    "counselling:setup": "npm run sqlite:setup && npm run counselling:import",
    "fix:college-name": "node fix-college-name.js",
    "cloudflare:setup": "wrangler d1 create college-database",
    "cloudflare:deploy": "wrangler deploy",
    "cloudflare:migrate": "wrangler d1 execute college-database --file=./schema.sql"
  },
  "keywords": [
    "college",
    "database",
    "sqlite",
    "cloudflare",
    "search",
    "medical",
    "dental",
    "dnb"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "express": "^4.18.2",
    "sqlite3": "^5.1.6",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.0.5",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "wrangler": "^3.0.0"
  }
}
